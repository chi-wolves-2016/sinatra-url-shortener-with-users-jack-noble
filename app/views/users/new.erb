<h1>Register Here!</h1>

  <form action="/users" method="POST">
    <br><label>User-Name:</label><br>
    <input type="text" name="user[username]" placeholder="Name Here">
    <br><label>E-Mail:</label><br>
    <input type="text" name="user[email]" placeholder="E-Mail Here">
    <br><label>Password:</label><br>
    <input type="password" name="user[password]" placeholder="Password Here">
    <button type="submit">Complete</button>
  </form>
